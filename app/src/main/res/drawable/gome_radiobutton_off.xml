<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@+id/state_off"
        android:state_checked="false"
        android:drawable="@drawable/radiobutton_00000" />

    <item
        android:id="@+id/state_on"
        android:drawable="@drawable/radiobutton_00012"
        android:state_checked="true" />

    <transition
        android:fromId="@id/state_on"
        android:toId="@id/state_off">
        <animation-list>
            <item
                android:drawable="@drawable/radiobutton_00012"
                android:duration="30" />
            <item
                android:drawable="@drawable/radiobutton_00011"
                android:duration="30" />
            <item
                android:drawable="@drawable/radiobutton_00010"
                android:duration="30" />
            <item
                android:drawable="@drawable/radiobutton_00009"
                android:duration="30" />
            <item
                android:drawable="@drawable/radiobutton_00008"
                android:duration="30" />
            <item
                android:drawable="@drawable/radiobutton_00007"
                android:duration="30" />
            <item
                android:drawable="@drawable/radiobutton_00006"
                android:duration="30" />
            <item
                android:drawable="@drawable/radiobutton_00005"
                android:duration="30" />
            <item
                android:drawable="@drawable/radiobutton_00004"
                android:duration="30" />
            <item
                android:drawable="@drawable/radiobutton_00003"
                android:duration="30" />
            <item
                android:drawable="@drawable/radiobutton_00002"
                android:duration="30" />
            <item
                android:drawable="@drawable/radiobutton_00001"
                android:duration="30" />
            <item
                android:drawable="@drawable/radiobutton_00000"
                android:duration="30" />
        </animation-list>
    </transition>
</animated-selector>